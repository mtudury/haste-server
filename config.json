{

  "host": "0.0.0.0",
  "port": 7777,

  "keyLength": 16,

  "maxLength": 4000000,

  "staticMaxAge": 86400,

  "recompressStaticAssets": true,

  "logging": [
    {
      "level": "verbose",
      "type": "Console",
      "colorize": true
    }
  ],

  "keyGenerator": {
    "type": "phonetic"
  },

  "rateLimits": {
    "categories": {
      "normal": {
        "totalRequests": 500,
        "every": 60000
      }
    }
  },

  "storage": {
    "type": "file",
    "allowList": true,
    "allowDelete": true
  },

  "documents": {
    "about.md": "./about.md"
  }

}
